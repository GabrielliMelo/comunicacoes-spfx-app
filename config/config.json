{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "comunicacoes-cliente-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/comunicacoesCliente/ComunicacoesClienteWebPart.js",
          "manifest": "./src/webparts/comunicacoesCliente/ComunicacoesClienteWebPart.manifest.json"
        }
      ]
    },
    "cleanpage-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/cleanpage/CleanpageApplicationCustomizer.js",
          "manifest": "./src/extensions/cleanpage/CleanpageApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ComunicacoesClienteWebPartStrings": "lib/webparts/comunicacoesCliente/loc/{locale}.js",
    "CleanpageApplicationCustomizerStrings": "lib/extensions/cleanpage/loc/{locale}.js"
  }
}
